{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login/(.*)", "dest": "/login/:userId" },
    { "src": "/dash", "dest": "https://javavnan-form-user.vercel.app/dash" },
    { "src": "/", "dest": "/" }
  ],
  "rewrites": [
    {
      "source": "/login/:userId",
      "destination": "https://javavnan-form-user.vercel.app/login/:userId"
    },
    {
      "source": "https://javavnan-form-user.vercel.app/dash",
      "destination": "https://javavnan-form-user.vercel.app"
    }
  ]
}
