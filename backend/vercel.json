{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login/(.*)", "dest": "/login/:userId" },
    { "src": "/dash", "dest": "/frontend/src/components/data/DeleteMe.jsx" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
