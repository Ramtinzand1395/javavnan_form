{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/dash",
      "destination": "/frontend/src/components/data/DeleteMe.jsx"
    }
  ]
}
