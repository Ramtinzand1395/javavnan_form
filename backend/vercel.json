{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login/:userId", "dest": "https://javavnan-form-user.vercel.app/login/:userId" },
    { "src": "/dash", "dest": "https://javavnan-form-user.vercel.app/dash" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
