{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login/(.*)", "dest": "/login/:userId" },
    { "src": "/dash", "dest": "/" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "rewrites": [
    {
      "source": "/login/:userId*",
      "destination": "https://https://javavnan-form-user.vercel.app/:login/:userId*"
    },
    {
      "source": "/dash*",
      "destination": "https://https://javavnan-form-user.vercel.app/dash*"
    }
  ]
}
